<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="symedit_events.symedit.menu.class">SymEdit\Bundle\CoreBundle\Menu\EventsBuilder</parameter>
    </parameters>

    <services>
        <service id="symedit_events.symedit.menu" class="%symedit_events.symedit.menu.class%">
            <tag name="symedit_menu.builder" menu="symedit_admin" />
        </service>
    </services>
</container>
